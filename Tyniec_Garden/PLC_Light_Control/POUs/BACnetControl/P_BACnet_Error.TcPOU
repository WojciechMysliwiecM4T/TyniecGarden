		stCommandBuffer:= GVL_IO.stCommandBuffer_RB3);
		
		//query first half of the lamp
fbDALIV2QueryLampFailure_9(
		bStart:= ton_Execute.Q, 
		nAddr:= GVL_IO.arrStreetLamps[9].nAddrShort1, 
		eAddrType:= eDALIV2AddrTypeShort, 
